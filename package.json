    "less": "4.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "semantic-release": "21.0.7",
    "sitemap": "7.1.1",
    "start-server-and-test": "2.0.0",
    "stream-to-array": "2.3.0",
